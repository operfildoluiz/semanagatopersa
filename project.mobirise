\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-md-12 content col-lg-6 m-auto\">\n                <h1 class=\"mbr-section-title align-left mbr-bold pb-5 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><a href=\"#\"><b>Por que </b></a>participar da Semana do Gato Persa</h1>\n\n\n\n                <div class=\"row\" mbr-if=\"showIcons\">\n\n                    <div class=\"col-md-6 card\" mbr-if=\"cards > 0\">\n\n                        <div class=\"icon\"> <span mbr-icon class=\"mbr-iconfont mbrib-hearth  \" mbr-if=\"showIcons\"></span>\n                        </div>\n\n                        <div class=\"text\">\n                            <h3 class=\"align-left mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                                Persas precisam de cuidados especiais de saúde</h3>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-6 card\" mbr-if=\"cards > 1\">\n\n                        <div class=\"icon\"> <span mbr-icon class=\"mbr-iconfont mobi-mbri-sun2 mobi-mbri\" mbr-if=\"showIcons\"></span>\n                        </div>\n\n                        <div class=\"text\">\n                            <h3 class=\"align-left mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\">A pelagem dos persas tem características específicas</h3>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-6 card\" mbr-if=\"cards > 2\">\n\n                        <div class=\"icon\"> <span mbr-icon class=\"mbr-iconfont mobi-mbri-github mobi-mbri\" mbr-if=\"showIcons\"></span> </div>\n\n                        <div class=\"text\">\n                            <h3 class=\"align-left mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\">Persas são super dóceis e exigem atenção e carinho</h3>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-6 card\" mbr-if=\"cards > 3\">\n\n                        <div class=\"icon\"> <span mbr-icon class=\"mbr-iconfont mobi-mbri-like mobi-mbri\" mbr-if=\"showIcons\"></span>\n                        </div>\n\n                        <div class=\"text\">\n                            <h3 class=\"align-left mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\">Todas as dicas e aulas são validadas por veterinários</h3>\n                        </div>\n                    </div>\n\n\n\n\n                </div>\n\n\n\n\n            </div>\n            <div class=\"col-md-12 col-lg-6 img-col\">\n                <div class=\"mbr-figure\">\n                    <img src=\"@PROJECT_PATH@/assets/images/stories-720x720.png\" alt>\n                </div>\n\n            </div>\n        </div>\n    </div>\n</section>",
